          <note xml:id="i-JmtHvKhjmrk7NnfrCDSpEz">
            Riksdagens protokoll
          </note>
          <u xml:id="i-ecc7d16002478aa6-0" who="unknown">
            <seg xml:id="i-2qRLJCMLE2vVk4hyaFcmdY">
              2010/11:18
