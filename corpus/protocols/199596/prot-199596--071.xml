        <div type="commentSection" xml:id="i-EB3PATKGm9smczv5MWBqvJ">
          <pb facs="http://data.riksdagen.se/fil/9B1A2D59-0ADF-4DBA-B6ED-7F85CF368A0F#page=1"/>
