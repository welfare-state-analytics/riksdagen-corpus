SELECT DISTINCT ?entity ?author ?titleLabel ?date ?subjectLabel ?url
WHERE
{
  ?entity wdt:P31/wdt:P279* wd:Q452237;wdt:P8433 [];
          wdt:P1476 ?title;
          wdt:P50 ?author;
          wdt:P577 ?date;
          wdt:P921 ?subject;
          wdt:P953 ?url.
  
  SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE],sv" } 
}