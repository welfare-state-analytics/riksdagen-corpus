SELECT ?wiki_id ?wiki_idLabel ?start ?end ?chamberLabel
WHERE
{
    ?wiki_id p:P39 ?posheld. # With position held
    VALUES ?chamber { wd:Q1850749 wd:Q85679583 wd:Q85679895 }
    ?posheld ps:P39 ?chamber. # Position held is in one of the chambers

    OPTIONAL { ?posheld pq:P580 ?start. }
    OPTIONAL { ?posheld pq:P582 ?end. }
  
  SERVICE wikibase:label { bd:serviceParam wikibase:language "sv". }
}