WHERE
{    
    ?wiki_id wdt:P31 wd:Q5;
             wdt:P12192 ?swerik_id.
    OPTIONAL { ?wiki_id skos:altLabel ?alias. filter(lang(?alias)="sv") }.
    {
    PLACEHOLDER
    }
  
  SERVICE wikibase:label { bd:serviceParam wikibase:language "sv". }
}
